{"ast":null,"code":"// @ is an alias to /src\nimport PostIt from \"@/components/PostIt.vue\";\nimport AddModale from \"@/components/AddModale.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    PostIt,\n    AddModale\n  },\n\n  data() {\n    return {\n      // id: 1,\n      posts: [],\n      showAddModale: false,\n      newTitle: \"\",\n      newContent: \"\"\n    };\n  },\n\n  methods: {\n    deletePostIt(id) {\n      this.posts = this.posts.filter(p => p.id != id);\n    },\n\n    showPostIt(id) {\n      this.$router.push({\n        name: \"detail\",\n        params: {\n          id: id\n        }\n      });\n    },\n\n    addPostIt() {\n      this.showAddModale = false;\n      this.posts.push({\n        id: this.id++,\n        title: this.newTitle,\n        content: this.newContent\n      });\n      this.newTitle = \"\";\n      this.newContent = \"\";\n      let headersPost = new Headers({\n        \"Content-Type\": \"application/json\"\n      });\n      let initPost = {\n        method: \"POST\",\n        body: JSON.stringify({\n          title: this.newTitle,\n          content: [this.newContent]\n        }),\n        headers: headersPost,\n        cache: \"default\"\n      };\n      fetch(\"http://5.135.119.239:3090/notes/\", initPost).then(res => res.json()).then(data => console.log(data));\n    },\n\n    updateTitle(id, newTitle) {\n      console.log(newTitle);\n      let index = this.posts.findIndex(function (post) {\n        return post.id == id;\n      });\n      this.posts[index].title = newTitle;\n    },\n\n    updateContent(id, newContent) {\n      let index = this.posts.findIndex(function (post) {\n        return post.id == id;\n      });\n      this.posts[index].content = newContent;\n    },\n\n    toggleAddModale() {\n      this.showAddModale = !this.showAddModale;\n      this.newTitle = \"\";\n      this.newContent = \"\";\n    }\n\n  },\n  watch: {\n    posts: {\n      handler(newPosts) {\n        localStorage.posts = JSON.stringify(newPosts);\n      },\n\n      deep: true\n    },\n    id: {\n      handler(newId) {\n        localStorage.id = newId;\n      }\n\n    }\n  },\n\n  mounted() {\n    // if (localStorage.posts) {\n    //   this.posts = JSON.parse(localStorage.posts);\n    // }\n    // if (localStorage.id) {\n    //   this.id = localStorage.id;\n    // }\n    let headersGet = new Headers({\n      \"Content-Type\": \"application/json\"\n    });\n    let initGet = {\n      method: \"GET\",\n      headers: headersGet,\n      mode: \"cors\",\n      cache: \"default\"\n    };\n    fetch(\"http://5.135.119.239:3090/notes\", initGet).then(res => res.json()).then(data => {\n      console.log(data);\n      console.log(data.notes); // data.forEach((d, index) => {\n      //   let post = { id: index, title: d.title, content: d.content[0] };\n      //   this.posts.push(post);\n      // });\n\n      for (let i = 0; i < data.length; i++) {\n        console.log(data[i].title);\n        let post = {\n          id: i,\n          title: data[i].title,\n          content: data[i].content[0]\n        };\n        this.posts.push(post);\n        console.log(this.posts);\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AACA;AACA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVH,UADU;AAEVC;AAFU,GAFC;;AAMbG,MAAI,GAAG;AACL,WAAO;AACL;AACAC,WAAK,EAAE,EAFF;AAGLC,mBAAa,EAAE,KAHV;AAILC,cAAQ,EAAE,EAJL;AAKLC,gBAAU,EAAE;AALP,KAAP;AAOD,GAdY;;AAebC,SAAO,EAAE;AACPC,gBAAY,CAACC,EAAD,EAAK;AACf,WAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAmBC,CAAD,IAAOA,CAAC,CAACF,EAAF,IAAQA,EAAjC,CAAb;AACD,KAHM;;AAIPG,cAAU,CAACH,EAAD,EAAK;AACb,WAAKI,OAAL,CAAaC,IAAb,CAAkB;AAChBd,YAAI,EAAE,QADU;AAEhBe,cAAM,EAAE;AAAEN,YAAE,EAAEA;AAAN;AAFQ,OAAlB;AAID,KATM;;AAUPO,aAAS,GAAG;AACV,WAAKZ,aAAL,GAAqB,KAArB;AACA,WAAKD,KAAL,CAAWW,IAAX,CAAgB;AACdL,UAAE,EAAE,KAAKA,EAAL,EADU;AAEdQ,aAAK,EAAE,KAAKZ,QAFE;AAGda,eAAO,EAAE,KAAKZ;AAHA,OAAhB;AAKA,WAAKD,QAAL,GAAgB,EAAhB;AACA,WAAKC,UAAL,GAAkB,EAAlB;AAE