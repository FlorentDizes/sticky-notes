      content: [this.newContent],\n        }),\n        headers: headersPost,\n        cache: \"default\",\n      };\n\n      fetch(\"http://5.135.119.239:3090/notes/\", initPost)\n        .then((res) => res.json())\n        .then((data) => console.log(data));\n    },\n\n    updateTitle(id, newTitle) {\n      console.log(newTitle);\n      let index = this.posts.findIndex(function (post) {\n        return post.id == id;\n      });\n      this.posts[index].title = newTitle;\n    },\n    updateContent(id, newContent) {\n      let index = this.posts.findIndex(function (post) {\n        return post.id == id;\n      });\n      this.posts[index].content = newContent;\n    },\n    toggleAddModale() {\n      this.showAddModale = !this.showAddModale;\n      this.newTitle = \"\";\n      this.newContent = \"\";\n    },\n  },\n  watch: {\n    posts: {\n      handler(newPosts) {\n        localStorage.posts = JSON.stringify(newPosts);\n      },\n      deep: true,\n    },\n    id: {\n      handler(newId) {\n        localStorage.id = newId;\n      },\n    },\n  },\n  mounted() {\n    // if (localStorage.posts) {\n    //   this.posts = JSON.parse(localStorage.posts);\n    // }\n    // if (localStorage.id) {\n    //   this.id = localStorage.id;\n    // }\n    let headersGet = new Headers({\n      \"Content-Type\": \"application/json\",\n    });\n    let initGet = {\n      method: \"GET\",\n      headers: headersGet,\n      mode: \"cors\",\n      cache: \"default\",\n    };\n    fetch(\"http://5.135.119.239:3090/notes\", initGet)\n      .then((res) => res.json())\n      .then((data) => {\n        console.log(data);\n        console.log(data.notes);\n        // data.forEach((d, index) => {\n        //   let post = { id: index, title: d.title, content: d.content[0] };\n        //   this.posts.push(post);\n        // });\n        for (let i = 0; i < data.length; i++) {\n          console.log(data[i].title);\n          let post = {\n            id: i,\n            title: data[i].title,\n  