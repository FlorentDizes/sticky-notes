sts[index].title = newTitle;\n    },\n    updateContent(id, newContent) {\n      let index = this.posts.findIndex(function (post) {\n        return post.id == id;\n      });\n      this.posts[index].content = newContent;\n    },\n    toggleAddModale() {\n      this.showAddModale = !this.showAddModale;\n      this.newTitle = \"\";\n      this.newContent = \"\";\n    },\n  },\n  watch: {\n    posts: {\n      handler(newPosts) {\n        localStorage.posts = JSON.stringify(newPosts);\n      },\n      deep: true,\n    },\n    id: {\n      handler(newId) {\n        localStorage.id = newId;\n      },\n    },\n  },\n  mounted() {\n    // if (localStorage.posts) {\n    //   this.posts = JSON.parse(localStorage.posts);\n    // }\n    // if (localStorage.id) {\n    //   this.id = localStorage.id;\n    // }\n    let headersGet = new Headers({\n      \"Content - Type\": \"application/json\",\n    });\n    let initGet = {\n      method: \"GET\",\n      headers: headersGet,\n      mode: \"cors\",\n      cache: \"default\",\n    };\n    fetch(\"http://5.135.119.239:3090\")\n      .then((res) => res.json)\n      .then((data) => console.log(data));\n  },\n};\n</script>\n\n<template>\n  <div class=\"container my-5\">\n    <AddModale\n      :showAddModale=\"showAddModale\"\n      :toggleAddModale=\"toggleAddModale\"\n      v-model:newTitle=\"newTitle\"\n      v-model:newContent=\"newContent\"\n      @submitData=\"addPostIt\"\n    />\n    <svg\n      class=\"addbtn\"\n      @click=\"toggleAddModale\"\n      xmlns=\"http://www.w3.org/2000/svg\"\n      viewBox=\"0 0 512 512\"\n    >\n      <!--! Font Awesome Pro 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2022 Fonticons, Inc. -->\n      <path\n        d=\"M0 256C0 114.6 114.6 0 256 0C397.4 0 512 114.6 512 256C512 397.4 397.4 512 256 512C114.6 512 0 397.4 0 256zM256 368C269.3 368 280 357.3 280 344V280H344C357.3 280 368 269.3 368 256C368 242.7 357.3 232 344 232H28