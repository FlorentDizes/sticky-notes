\n      method: \"GET\",\n      headers: headersGet,\n      mode: \"cors\",\n      cache: \"default\"\n    };\n    fetch(\"http://5.135.119.239:3090\").then(res => res.json).then(data => console.log(data));\n  }\n\n};","map":{"version":3,"mappings":"AACA;AACA,OAAOA,MAAP,MAAmB,yBAAnB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVH,UADU;AAEVC;AAFU,GAFC;;AAMbG,MAAI,GAAG;AACL,WAAO;AACL;AACAC,WAAK,EAAE,EAFF;AAGLC,mBAAa,EAAE,KAHV;AAILC,cAAQ,EAAE,EAJL;AAKLC,gBAAU,EAAE;AALP,KAAP;AAOD,GAdY;;AAebC,SAAO,EAAE;AACPC,gBAAY,CAACC,EAAD,EAAK;AACf,WAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAmBC,CAAD,IAAOA,CAAC,CAACF,EAAF,IAAQA,EAAjC,CAAb;AACD,KAHM;;AAIPG,cAAU,CAACH,EAAD,EAAK;AACb,WAAKI,OAAL,CAAaC,IAAb,CAAkB;AAChBd,YAAI,EAAE,QADU;AAEhBe,cAAM,EAAE;AAAEN,YAAE,EAAEA;AAAN;AAFQ,OAAlB;AAID,KATM;;AAUPO,aAAS,GAAG;AACV,WAAKZ,aAAL,GAAqB,KAArB;AACA,WAAKD,KAAL,CAAWW,IAAX,CAAgB;AACdL,UAAE,EAAE,KAAKA,EAAL,EADU;AAEdQ,aAAK,EAAE,KAAKZ,QAFE;AAGda,eAAO,EAAE,KAAKZ;AAHA,OAAhB;AAKA,WAAKD,QAAL,GAAgB,EAAhB;AACA,WAAKC,UAAL,GAAkB,EAAlB;AACD,KAnBM;;AAoBPa,eAAW,CAACV,EAAD,EAAKJ,QAAL,EAAe;AACxBe,aAAO,CAACC,GAAR,CAAYhB,QAAZ;AACA,UAAIiB,KAAI,GAAI,KAAKnB,KAAL,CAAWoB,SAAX,CAAqB,UAAUC,IAAV,EAAgB;AAC/C,eAAOA,IAAI,CAACf,EAAL,IAAWA,EAAlB;AACD,OAFW,CAAZ;AAGA,WAAKN,KAAL,CAAWmB,KAAX,EAAkBL,KAAlB,GAA0BZ,QAA1B;AACD,KA1BM;;AA2BPoB,iBAAa,CAAChB,EAAD,EAAKH,UAAL,EAAiB;AAC5B,UAAIgB,KAAI,GAAI,KAAKnB,KAAL,CAAWoB,SAAX,CAAqB,UAAUC,IAAV,EAAgB;AAC/C,eAAOA,IAAI,CAACf,EAAL,IAAWA,EAAlB;AACD,OAFW,CAAZ;AAGA,WAAKN,KAAL,CAAWmB,KAAX,EAAkBJ,OAAlB,GAA4BZ,UAA5B;AACD,KAhCM;;AAiCPoB,mBAAe,GAAG;AAChB,WAAKtB,aAAL,GAAqB,CAAC,KAAKA,aAA3B;AACA,WAAKC,QAAL,GAAgB,EAAhB;AACA,WAAKC,UAAL,GAAkB,EAAlB;AACD;;AArCM,GAfI;AAsDbqB,OAAK,EAAE;AACLxB,SAAK,EAAE;AACLyB,aAAO,CAACC,QAAD,EAAW;AAChBC,oBAAY,CAAC3B,KAAb,GAAqB4B,IAAI,CAACC,SAAL,CAAeH,QAAf,CAArB;AACD,OAHI;;AAILI,UAAI,EAAE;AAJD,KADF;AAOLxB,MAAE,EAAE;AACFmB,aAAO,CAACM,KAAD,EAAQ;AACbJ,oBAAY,CAACrB,EAAb,GAAkByB,KAAlB;AACD;;AAHC;AAPC,GAtDM;;AAmEbC,SAAO,GAAG;AACR;AACA;AACA;AACA;AACA;AACA;