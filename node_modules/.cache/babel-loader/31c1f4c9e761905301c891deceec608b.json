ePostIt(id) {\n      this.posts = this.posts.filter((p) => p.id != id);\n    },\n    showPostIt(id) {\n      this.$router.push({ name: \"detail\", params: { id: id } });\n    },\n    addPostIt() {\n      console.log(this.newTitle);\n      this.showAddModale = false;\n      this.posts.push({ id: 7, title: this.newTitle, content: \"Content 7\" });\n    },\n    toggleAddModale() {\n      this.showAddModale = !this.showAddModale;\n    },\n  },\n};\n</script>\n\n<template>\n  <div class=\"container my-5\">\n    <AddModale\n      :showAddModale=\"showAddModale\"\n      :toggleAddModale=\"toggleAddModale\"\n      :newTitle=\"newTitle\"\n      :newContent=\"newContent\"\n      @submitData=\"addPostIt\"\n    />\n    <div @click=\"toggleAddModale\" class=\"btn btn-success\">Add Post-It</div>\n  </div>\n  <div class=\"postits\">\n    <PostIt\n      v-for=\"post in posts\"\n      :key=\"post.id\"\n      :id=\"post.id\"\n      :title=\"post.title\"\n      :content=\"post.content\"\n      @deletePostIt=\"deletePostIt\"\n      @showPostIt=\"showPostIt\"\n    />\n  </div>\n</template>\n\n<style scoped>\n.postits {\n  display: flex;\n  flex-wrap: wrap;\n}\n</style>\n\n\n"]},"metadata":{},"sourceType":"module"}